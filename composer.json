{
  "name": "medaqueno/boilerwork-kernel",
  "description": "Boilerwork Kernel",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2.0",
    "beberlei/assert": "^3.3",
    "doctrine/dbal": "^3.5",
    "doctrine/migrations": "^3.5",
    "elasticsearch/elasticsearch": "^8.6",
    "giggsey/libphonenumber-for-php": "^8.12",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/container": "^9.4",
    "laminas/laminas-diactoros": "^2.8",
    "nikic/fast-route": "^1.3",
    "nikic/iter": "^2.2",
    "openswoole/core": "^22.1",
    "php-ds/php-ds": "^1.4",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0.1",
    "psr/log": "^1.0 || ^2.0",
    "psr/simple-cache": "^1.0",
    "sentry/sdk": "^3.3",
    "symfony/uid": "^6.2",
    "vlucas/phpdotenv": "^5.4",
    "symfony/var-dumper": "^6.2"
  },
  "require-dev": {
    "deminy/counit": "^0.2.0",
    "openswoole/ide-helper": "^22",
    "phpunit/phpunit": "^9.5",
    "ukko/phpredis-phpdoc": "5.0.1-beta8",
    "kwn/php-rdkafka-stubs": "^2.2",
    "symfony/var-dumper": "^6.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "autoload": {
    "psr-4": {
      "Boilerwork\\": "src/Boilerwork/"
    },
    "files": [
      "src/Boilerwork/Support/functions.php"
    ]
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit --color --testdox tests"
    ]
  },
  "authors": [
    {
      "name": "CarlosJ - medaqueno"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
